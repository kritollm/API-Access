{
  "name": "API Access",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Allows to you request any API with ajax from any source.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/enable_16.png",
    "48": "icons/enable_48.png",
    "128": "icons/enable_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/disable_19.png",
      "38": "icons/disable_38.png"
    },
    "default_title": "Allow CORS"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "messagehandler.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "icons/enable_19.png",
    "icons/disable_19.png",
    "icons/enable_38.png",
    "icons/disable_38.png",
    "enablecrossorigin.js",
    "injector.js"
  ]
}